               <entry key="6" value-ref="renterChargeOrderPayService"/>
           </map>
       </property>
    </bean>
    <bean id="rechargeOrderPayService" class="com.edianniu.pscp.mis.service.orderpay.impl.RechargeOrderPayService">
       
