    <bean id="socialElectricianWorkOrderPayService" class="com.edianniu.pscp.mis.service.orderpay.impl.SocialElectricianWorkOrderPayService">
      
    </bean>
    <bean id="needsOrderPayService" class="com.edianniu.pscp.mis.service.orderpay.impl.NeedsOrderPayService">
     
    </bean>
    <bean id="projectOrderPayService" class="com.edianniu.pscp.mis.service.orderpay.impl.ProjectOrderPayService">
       
    </bean>
    <bean id="renterChargeOrderPayService" class="com.edianniu.pscp.mis.service.orderpay.impl.RenterChargeOrderPayService">
       
    </bean>
</beans>