<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="orderPayService" class="com.edianniu.pscp.mis.service.orderpay.impl.DefaultOrderPayService">
       <property name="serviceMap">
           <map>
               <entry key="1" value-ref="rechargeOrderPayService"/>
               <entry key="2" value-ref="socialWorkOrderPayService"/>
               <entry key="3" value-ref="socialElectricianWorkOrderPayService"/>
